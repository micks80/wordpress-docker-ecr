node {
  stage 'Checkout'
  git 'https://github.com/micks80/wordpress-docker-ecr.git'

  stage 'Package Docker image'
  def img = docker.build('dockpress:latest', '.')

  stage 'Publish'

docker.withRegistry('https://560414214996.dkr.ecr.us-east-1.amazonaws.com/dockpress', 'ecr:docker-ecr') {
    img.push('latest')
  }
}
